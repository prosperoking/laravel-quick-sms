{
    "name": "prosperoking/laravel-quick-sms",
    "description": "A simple quicksms sender for Laravel",
    "type": "library",
    "version":"0.2.1",
    "keywords": [
        "laravel",
        "smslive247",
        "quicksms",
        "prosperoking"
    ],
    "require": {
        "illuminate/support": "^5.4",
        "guzzlehttp/guzzle": "^6.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Prosper Nweze",
            "email": "daponextraspp@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Prosperoking\\LaravelQuickSms\\":"src"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Prosperoking\\LaravelQuickSms\\QuickSMSServiceProvider"
            ],
            "aliases": {
                "QuickSMS": "Prosperoking\\LaravelQuickSms\\Facades\\QuickSMSFacade"
            }
        }
    }
}

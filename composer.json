{
    "name": "prosperoking/quick-sms",
    "description": "A simple quicksms sender for Laravel",
    "type": "Laravel package",
    "require": {
        "illuminate/support": "^5.4",
        "guzzlehttp/guzzle": "^6.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Prosper Name",
            "email": "daponextraspp@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Prosperoking\\QuickSms\\":"src"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Proseperoking\\QuickSMS\\ServiceProvider"
            ],
            "aliases": {
                "QuickSMS": "Proseperoking\\QuickSMS\\Facade"
            }
        }
    }
}
